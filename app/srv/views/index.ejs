<!DOCTYPE html>
<html lang="ru">
<head>
    <base target="_blank">
    <% if (helmet) {%>
        <%- helmet.title.toString() %>
        <%- helmet.meta.toString() %>
        <% if (helmet.link) {%> <%- helmet.link.toString() %> <%} %>
    <%}%>
    <link href="/static/bundle.css" rel="stylesheet">
    <script type="text/javascript" src="/static/vendor.js"></script>
</head>
<body>
    <div id="app"></div>
    <div id="app_server"><%- reactData %></div>
    <div id="vk_api_transport"></div>


    <script>window.twttr = (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0],
        t = window.twttr || {};
        if (d.getElementById(id)) return t;
        js = d.createElement(s);
        js.id = id;
        js.src = "https://platform.twitter.com/widgets.js";
        fjs.parentNode.insertBefore(js, fjs);

        t._e = [];
        t.ready = function(f) {
        t._e.push(f);
        };
        return t;
        }(document, "script", "twitter-wjs"));
    </script>
    <div id="vk_api_transport"></div>
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <script src="//platform.instagram.com/en_US/embeds.js"></script>
    <script type="text/javascript" src="/static/bundle.js"></script>
    <%- Object.keys(settings) %>
    <%- settings.yandexId %>

    <% if (settings.YANDEX_METRIKA_ID) {%>
        <!-- Yandex.Metrika counter -->
        <script type="text/javascript">
            (function (d, w, c) {
                (w[c] = w[c] || []).push(function() {
                    try {
                        w.yaCounter<%- settings.YANDEX_METRIKA_ID%> = new Ya.Metrika({
                            id:<%- settings.YANDEX_METRIKA_ID%>,
                            clickmap:true,
                            trackLinks:true,
                            accurateTrackBounce:true,
                            webvisor:true
                        });
                    } catch(e) { }
                });

                var n = d.getElementsByTagName("script")[0],
                    s = d.createElement("script"),
                    f = function () { n.parentNode.insertBefore(s, n); };
                s.type = "text/javascript";
                s.async = true;
                s.src = "https://mc.yandex.ru/metrika/watch.js";

                if (w.opera == "[object Opera]") {
                    d.addEventListener("DOMContentLoaded", f, false);
                } else { f(); }
            })(document, window, "yandex_metrika_callbacks");
        </script>
        <noscript><div><img src="https://mc.yandex.ru/watch/43842599" style="position:absolute; left:-9999px;" alt="" /></div></noscript>
        <!-- /Yandex.Metrika counter -->
    <%}%>
    <% if (settings.GOOGLE_ANALYTICS_ID) {%>
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

          ga('create', '<%- settings.GOOGLE_ANALYTICS_ID %>', 'auto');
          ga('send', 'pageview');

        </script>
    <%}%>
</body>
</html>